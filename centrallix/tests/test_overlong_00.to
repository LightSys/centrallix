##NAME overlong function

# 1 byte chars
query select overlong("a valid string")
query select overlong("For God so loved the world, that he gave his only Son, that whoever believes in him should not perish but have eternal life.")
query select overlong("invalid")

# 2 byte chars
query select overlong("допустимая строка")
query select overlong("Ведь Бог так полюбил этот мир, что отдал Своего единственного Сына, чтобы каждый верующий в Него не погиб, но имел вечную жизнь.")
query select overlong("инвалид")

# 3 byte chars 
query select overlong("有効な文字列")
query select overlong("それ神はその獨子を賜ふほどに世を愛し給へり、すべて彼を信ずる者の亡びずして、永遠の生命を得んためなり。")
query select overlong("無効")

# 4 byte chars
query select overlong("𓅀𓅁𓅂𓅃𓅄𓅅𓅆𓅇𓅈")
query select overlong("𓀀𓀁𓀂𓀃𓀄𓀅𓀆𓀇𓀈𓀉𓀊𓀋𓀌𓀍𓀎𓀏𓀐𓀑𓀒𓀓𓀔𓀕𓀖𓀗𓀘𓀙𓀚𓀛𓀜𓀝𓀞𓀟𓀠𓀡𓀢𓀣𓀤𓀥𓀦𓀧𓀨𓀩𓀪𓀫𓀬𓀭𓀮𓀯𓀰𓀱𓀲𓀳𓀴𓀵𓀶𓀷𓀸𓀹𓀺𓀻𓀼𓀽𓀾")
query select overlong("𓂀𓂁𓂂𓂃𓂄")

