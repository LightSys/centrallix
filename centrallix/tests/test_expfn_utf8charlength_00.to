##NAME char_length() function

query select 'utf8_char_length("abc")' = char_length("abc")
query select 'utf8_char_length("a")' = char_length("a")
query select 'utf8_char_length("")' = char_length("")
query select 'utf8_char_length(null)' = char_length(null)
query select 'utf8_char_length("abcde"*999999)' = char_length("abcde" * 999999)
query select 'utf8_char_length("كتاب")' = char_length("كتاب")
query select 'utf8_char_length(:String) from /tests/Foreign.csv/rows' = utf8_char_length(:String) from /tests/Foreign.csv/rows
