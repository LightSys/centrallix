query select 'ralign("测试", -4)' = ralign("测试", -4)
query select 'ralign("Prüfung", 20)' = ralign("Prüfung", 20)
query select 'ralign("テスト テスト テスト テスト", 100)' = ralign("テスト テスト テスト テスト", 100)
