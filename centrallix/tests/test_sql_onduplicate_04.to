##NAME ON DUP clause, insert and update
##UNDO /tests/Testdata1.csv

csv select * from /tests/Testdata1.csv/rows
query insert into /tests/Testdata1.csv/rows select f_id = :f_id + 6, :f_desc, :f_count from /tests/Testdata1.csv/rows t1 where :f_id < 4 on duplicate :f_id update set :f_count = :f_count + :t1:f_count
csv select * from /tests/Testdata1.csv/rows
