##NAME constrain() function

query select 'constrain(0,-1,1)' = constrain(0,-1,1)

query select 'constrain(0,1,2)' = constrain(0,1,2)

query select 'constrain(0,-2,-1)' = constrain(0,-2,-1)

query select 'constrain(0.0,-1.0,1.0)' = constrain(0.0,-1.0,1.0)

query select 'constrain(0.0,1.0,2.0)' = constrain(0.0,1.0,2.0)

query select 'constrain(0.0,-2.0,-1.0)' = constrain(0.0,-2.0,-1.0)

query select 'constrain($0.0,$-1.0,$1.0)' = constrain($0.0,$-1.0,$1.0)

query select 'constrain($0.0,$1.0,$2.0)' = constrain($0.0,$1.0,$2.0)

query select 'constrain($0.0,$-2.0,$-1.0)' = constrain($0.0,$-2.0,$-1.0)



