##NAME Datetime comparisons
query select :d:f_datetime, equal_to=(select sum(convert(string,:d2:f_datetime) + ',') from /tests/Datatypes.csv/rows d2 where :d2:f_datetime = :d:f_datetime), less_than=(select sum(convert(string,:d2:f_datetime) + ',') from /tests/Datatypes.csv/rows d2 where :d2:f_datetime < :d:f_datetime), greater_than=(select sum(convert(string,:d2:f_datetime) + ',') from /tests/Datatypes.csv/rows d2 where :d2:f_datetime > :d:f_datetime) from /tests/Datatypes.csv/rows d
