$Version=2$
app_diagram "system/report"
    {
    title = "Application Structure Report";
    document_format="application/pdf";
    resolution = 300;
    pagewidth=85;
    pageheight=66;
    marginleft=2.5;
    marginright=2.5;
    margintop=3;
    marginbottom=3;

    app_path "report/parameter" { type=string; default="/samples/component_test.app"; }
    show_sys_cmp "report/parameter" { type=integer; default=0; }
    }
